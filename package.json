{
  "name": "authyntic-demo",
  "version": "1.0.0",
  "description": "Interactive web demo of the Authyntic media provenance platform",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "npm run build:lib && vite build",
    "build:lib": "tsc -p tsconfig.build.json && node scripts/prepare-build.cjs",
    "preview": "vite preview",
    "test": "npm run build:lib && node --test --experimental-test-coverage tests/**/*.test.cjs"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "crypto-js": "^4.2.0",
    "d3": "^7.8.5",
    "recharts": "^2.8.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@types/d3": "^7.4.3",
    "@vitejs/plugin-react": "^4.1.0",
    "typescript": "^5.2.2",
    "vite": "^4.4.9"
  }
}